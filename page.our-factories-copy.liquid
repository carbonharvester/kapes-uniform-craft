    "8be4e314-3cf3-430b-aba7-254ed5b7f210": {
      "type": "image-with-text",
      "blocks": {
        "template--19950547370300__8be4e314-3cf3-430b-aba7-254ed5b7f210-1689054962738e24cb-0": {
